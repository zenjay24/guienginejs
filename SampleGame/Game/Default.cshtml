<script src="~/Scripts/jquery-1.7.1.js"></script>
<script src="~/scripts/guiengine.js"></script>
<script>
    var gamestr,
        gameimg,
        canvas,
        game,
        gameObjs;

    $(document).ready(function () {
        game = guiEngine.start();
        game.SetDebugger("gedebugger");
        game.Debug(true);
        canvas = game.getCanvas("gameCanvas").setBackgroundColor("#000000");
        $('#createImageData').bind('click', function () { game.CreateImageData("gameCanvas"); });
        gameObjs = canvas.bindGEObjects();
    });
</script>

<canvas id="gameCanvas" data-gengine='{"handleEvents":["mouse", "touch"]}' width="1280" height="720">
</canvas>
<GEString id='string1' data-gengine='{"canvasId":"gameCanvas","color":"#FFF00F","x":200,"y":400,"content":"I Love"}'></GEString>
